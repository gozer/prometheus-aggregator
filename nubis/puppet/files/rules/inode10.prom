ALERT Inode10
  IF node_filesystem_files_free{mountpoint="/"} / node_filesystem_files{mountpoint="/"} * 100 < 10
  FOR 5m
  LABELS {
    severity="critical",
    type="inode-usage",
    type="meta-failure"
  }
  ANNOTATIONS {
    summary = "",
    description = "Inode percentage free is {{ $value }} < 10%"
  }